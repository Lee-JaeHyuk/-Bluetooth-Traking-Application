<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5fA"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f5f5fA"
        android:gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        android:text="Saved Pairing Data"
        android:textColor="#290429"
        android:textSize="19dp"
        android:textStyle="bold" />

    <TableLayout
        android:id="@+id/table1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:stretchColumns="*"
        android:shrinkColumns="*">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:layout_marginBottom="0.5dp"
            android:background="#000000">

            <Button
                android:id="@+id/bt1"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                android:layout_margin="1dp"
                android:background="#FFFFD0"
                android:gravity="center"
                android:text="Pairing Start 1"
                android:textColor="#000000"
                android:textSize="19.5dp"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:layout_marginBottom="1dp">

        <TextView
            android:id="@+id/txtStartAddress1"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFE0"
            android:gravity="center"
            android:text="주소"
            android:textColor="#000000"
            android:textSize="17dp"
            android:textStyle="bold" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:layout_marginBottom="0.5dp">

        <TextView
            android:id="@+id/txtStartAddress2"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFF9"
            android:gravity="center"
            android:textColor="#000"
            android:textSize="15dp"
            android:text="Address"/>

    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0.5dp"
        android:background="#000000">

        <TextView
            android:id="@+id/txtStartAddress3"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFE0"
            android:gravity="center"
            android:text="시간"
            android:textColor="#000000"
            android:textSize="17dp"
            android:textStyle="bold" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0.5dp"
        android:background="#000000">

        <TextView
            android:id="@+id/txtStartAddress4"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFF9"
            android:gravity="center"
            android:textColor="#000"
            android:textSize="15dp"
            android:text="Time" />

    </TableRow>

    <TableLayout
        android:id="@+id/table2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:stretchColumns="*"
        android:shrinkColumns="*">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:background="#000000">

            <Button
                android:id="@+id/bt2"
                android:layout_width="101dp"
                android:layout_height="match_parent"
                android:layout_margin="1dp"
                android:background="#FFFFD0"
                android:gravity="center"
                android:text="Pairing End 1"
                android:textColor="#000000"
                android:textSize="19.5dp"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:layout_marginBottom="0.5dp">

        <TextView
            android:id="@+id/txtStartAddress5"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFE0"
            android:gravity="center"
            android:text="주소"
            android:textColor="#000000"
            android:textSize="17dp"
            android:textStyle="bold" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:layout_marginBottom="0.5dp">

        <TextView
            android:id="@+id/txtStartAddress6"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFF9"
            android:gravity="center"
            android:textColor="#000"
            android:textSize="15dp"
            android:text="Address"/>

    </TableRow>

    <TableRow
        android:id="@+id/tableRow7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0.5dp"
        android:background="#000000">

        <TextView
            android:id="@+id/txtStartAddress7"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFE0"
            android:gravity="center"
            android:text="시간"
            android:textColor="#000000"
            android:textSize="17dp"
            android:textStyle="bold" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0.5dp"
        android:background="#000000">

        <TextView
            android:id="@+id/txtStartAddress8"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="0.5dp"
            android:background="#FFFFF9"
            android:gravity="center"
            android:textColor="#000"
            android:textSize="15dp"
            android:text="Time" />

    </TableRow>

</LinearLayout>